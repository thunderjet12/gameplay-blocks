{
  "name": "gameplay-blocks",
  "version": "1.0.0",
  "description": "A foundation for WordPress Gutenberg blocks plugin development that aims to provide clear and concise guide for building your first block plugin.",
  "main": "index.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start": "cross-env NODE_ENV=development  webpack --color --config resources/build/webpack.development.ts",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel resources/scripts --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build": "cross-env WPEMERGE_COMBINED_BUILD=1 run-s -l build:debug build:production",
    "build:debug": "cross-env NODE_ENV=debug webpack --color --config resources/build/webpack.production.ts",
    "build:production": "cross-env NODE_ENV=production webpack --color --config resources/build/webpack.production.ts",
    "lint": "run-s -cl lint:wpcs lint:scripts lint:styles",
    "lint-fix": "run-s -cl lint-fix:wpcs lint-fix:scripts lint-fix:styles",
    "lint:wpcs": "./vendor/bin/phpcs --color --standard=phpcs.xml ./",
    "lint:scripts": "eslint --color ./resources/scripts/**",
    "lint:styles": "stylelint --color ./resources/styles/**",
    "lint-fix:wpcs": "./vendor/bin/phpcbf --color --standard=wpcs.xml ./",
    "lint-fix:scripts": "eslint --color --fix ./resources/scripts/**",
    "lint-fix:styles": "stylelint --color --fix ./resources/styles/**",
    "i18n": "run-s -cl i18n:textdomain i18n:pot",
    "i18n:textdomain": "wpi18n addtextdomain --exclude=dist,node_modules,resources,tests,vendor --textdomain=my_app",
    "i18n:pot": "wpi18n makepot --exclude=dist,node_modules,resources,tests,vendor --type=wp-plugin --domain-path=languages/ --pot-file=my_app.pot --textdomain=my_app",
    "rebrand": "node resources/build/rebrand/rebrand.js",
    "release": "run-s build release:zip",
    "release:zip": "node resources/build/release/release.js wpemerge-release",
    "validate": "svelte-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phpbits/block-builder.git"
  },
  "keywords": [
    "gutenberg",
    "blocks",
    "WordPress",
    "tools"
  ],
